{
  "name": "bi-uploader",
  "version": "0.1.0",
  "description": "NDJSON queue and Snowpipe uploader for ActivitySchema events",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "tsx watch src/index.ts",
    "start:test": "NODE_ENV=test tsx src/index.ts",
    "test": "jest",
    "test:chaos": "tsx tests/chaos.test.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "p-queue": "^7.4.1",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3",
    "rotating-file-stream": "^3.1.1",
    "snowflake-sdk": "^1.9.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "snowflake",
    "snowpipe",
    "streaming",
    "ndjson",
    "activityschema"
  ],
  "author": "",
  "license": "MIT"
}
